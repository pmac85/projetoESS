<% provide(:title, 'All teams') %>
<div class="divs">
  <h1>All teams</h1>

  <%= will_paginate %>
  <table class="table table-hover table-striped">
    <thead>
      <tr class="info">
        <th>Name</th>
        <th>UserID</th>
        <th>LeagueID</th>
        <th>Action</th>
      </tr>
    </thead>

    <tbody>
      <%= render @teams %>
    </tbody>
  </table>
  <%= will_paginate %>
</div>